

<% unless !user_signed_in? %>

<h2><%= current_user.name %>'s posts </h2>
  <% @posts.each do |p| %>
    <div class="post">
        <ul>
          <p>
            <li><%= link_to p.title, post_path(current_user.posts) %></li> | By: <%= p.user.name %>
          </p>
        </ul>
    </div>
  <% end %>
<% end %>


<% if @posts.nil? %>
      <h2>It looks like you have not created any posts yet. Please click the New Post on the menu bar.</h2>
<% end %>

