<h1>Log In</h1>

<%= form_for(@user) do |f| %>
    <div class="field">
        <%= f.label :username %>
        <%= f.text_field :username %>
    </div><br>

    <div class="field">
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div><br>
    
    <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div><br>

        <%= f.submit "Log In" %>
    
<% end %>